{
  "metadata": [
    {
      "src": [
        {
          "files": ["BlazorFocused.Core.csproj"],
          "src": "../../BlazorFocused/src/core"
        },
        {
          "files": ["BlazorFocused.Tools.csproj"],
          "src": "../../BlazorFocused/src/tools"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net6.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["guides/**.md", "guides/**/toc.yml", "toc.yml", "*.md"],
        "exclude": ["README.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**", "CNAME"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "build/**"]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Documentation",
      "_appLogoPath": "images/BlazorFocused.png"
    },
    "dest": "build",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
